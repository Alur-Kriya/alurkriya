<script>
  const brandIcon = document.getElementById('brand-icon');
  const brandLogo = document.getElementById('brand-logo');

  const updateBrand = () => {
    if (window.scrollY > 50) {
      // Scrolled: Hide Icon, Show Logo
      brandIcon?.classList.add('opacity-0', 'translate-x-2', 'absolute');
      brandIcon?.classList.remove('opacity-100', 'translate-x-0');

      brandLogo?.classList.remove('opacity-0', '-translate-x-2', 'absolute');
      brandLogo?.classList.add('opacity-100', 'translate-x-0');
    } else {
      // Top: Show Icon, Hide Logo
      brandIcon?.classList.remove('opacity-0', 'translate-x-2', 'absolute');
      brandIcon?.classList.add('opacity-100', 'translate-x-0');

      brandLogo?.classList.add('opacity-0', '-translate-x-2', 'absolute');
      brandLogo?.classList.remove('opacity-100', 'translate-x-0');
    }
  };

  window.addEventListener('scroll', updateBrand);
  updateBrand(); // Jalankan saat inisialisasi untuk menangani refresh halaman di posisi scroll
</script>
