---
import Switcher from '@components/ui/Switcher.astro';
import Icon from '@components/ui/Icon.astro';
import Logo from '@components/ui/Logo.astro';
---

<header
  class="sticky top-0 inset-x-0 flex flex-wrap md:justify-start md:flex-nowrap z-50 text-sm"
>
  <nav
    class="mt-4 relative max-w-5xl w-full flex flex-wrap md:flex-nowrap basis-full items-center justify-between py-2 ps-5 pe-2 md:py-0 mx-2 lg:mx-auto rounded-[26px] bg-white dark:bg-(--color-deep-space-blue-950)"
  >

      <div class="flex items-center">
        <a
          class="flex-none text-xl font-semibold focus:opacity-80 focus:outline-hidden dark:text-neutral-300"
          href="/"
          aria-label="Brand"
        >
          <span
            id="brand-icon"
            class="mt-2 inline-block translate-x-0 transform opacity-100 transition-all duration-500 ease-in-out"
          >
            <Icon />
          </span>
          <span
            id="brand-logo"
            class="mt-2 absolute inline-block -translate-x-2 transform opacity-0 transition-all duration-500 ease-in-out"
          >
            <Logo />
          </span>
        </a>
      </div>

      <div class="flex items-center gap-1 md:order-4 md:ms-4">
        <!-- Collapse Button -->
        <button
          type="button"
          class="hs-collapse-toggle relative flex size-9 items-center justify-center rounded-lg border border-gray-200 text-sm font-medium hover:bg-gray-100 focus:bg-gray-100 focus:outline-hidden disabled:pointer-events-none disabled:opacity-50 md:hidden dark:border-neutral-700 dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
          id="hs-header-base-collapse"
          aria-expanded="false"
          aria-controls="hs-header-base"
          aria-label="Toggle navigation"
          data-hs-collapse="#hs-header-base"
        >
          <svg
            class="hs-collapse-open:hidden size-4"
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            fill="none"
            stroke="currentColor"
            stroke-width="2"
            stroke-linecap="round"
            stroke-linejoin="round"
            ><line x1="3" x2="21" y1="6" y2="6"></line><line x1="3" x2="21" y1="12" y2="12"
            ></line><line x1="3" x2="21" y1="18" y2="18"></line></svg
          >
          <svg
            class="hs-collapse-open:block hidden size-4 shrink-0"
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            fill="none"
            stroke="currentColor"
            stroke-width="2"
            stroke-linecap="round"
            stroke-linejoin="round"><path d="M18 6 6 18"></path><path d="m6 6 12 12"></path></svg
          >
          <span class="sr-only">Toggle navigation</span>
        </button>
        <!-- End Collapse Button -->
      </div>

    <!-- Collapse -->
    <div
      id="hs-header-base"
      class="hs-collapse hidden grow basis-full overflow-hidden transition-all duration-300 md:block"
      aria-labelledby="hs-header-base-collapse"
    >
      <div
        class="max-h-[75vh] overflow-hidden overflow-y-auto [&::-webkit-scrollbar]:w-2 [&::-webkit-scrollbar-thumb]:rounded-full [&::-webkit-scrollbar-thumb]:bg-gray-300 dark:[&::-webkit-scrollbar-thumb]:bg-neutral-500 [&::-webkit-scrollbar-track]:bg-gray-100 dark:[&::-webkit-scrollbar-track]:bg-neutral-700"
      >
        <div class="flex flex-col gap-0.5 py-2 md:flex-row md:items-center md:gap-1 md:py-0">
          <div class="grow">
            <div class="flex flex-col font-(family-name:--font-heading) gap-0.5 md:flex-row md:items-center md:justify-end md:gap-1">
              <a
                class="text-(--color-ink-black-800)hover:bg-gray-100 flex items-center rounded-lg p-2 text-sm font-semibold focus:bg-gray-100 focus:outline-hidden dark:bg-neutral-700 dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
                href="/"
              >
                <svg
                  class="me-3 block size-4 shrink-0 md:me-2 md:hidden"
                  xmlns="http://www.w3.org/2000/svg"
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="2"
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  ><path d="M15 21v-8a1 1 0 0 0-1-1h-4a1 1 0 0 0-1 1v8"></path><path
                    d="M3 10a2 2 0 0 1 .709-1.528l7-5.999a2 2 0 0 1 2.582 0l7 5.999A2 2 0 0 1 21 10v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"
                  ></path></svg
                >
                Beranda
              </a>

              <!-- Mega Menu -->
              <div
                class="hs-dropdown [--strategy:static] md:[--strategy:absolute] [--adaptive:none] md:[--trigger:hover] [--auto-close:inside] md:inline-block open"
              >
                <button
                  id="hs-header-base-mega-menu-fullwidth"
                  type="button"
                  class="hs-dropdown-toggle flex w-full items-center rounded-lg p-2 text-sm font-semibold text-(--color-ink-black-800) hover:bg-gray-100 focus:bg-gray-100 focus:outline-hidden dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
                  aria-haspopup="menu"
                  aria-expanded="false"
                  aria-label="Mega Menu"
                >
                  <svg
                    class="me-3 block size-4 shrink-0 md:me-2 md:hidden"
                    xmlns="http://www.w3.org/2000/svg"
                    width="24"
                    height="24"
                    viewBox="0 0 24 24"
                    fill="none"
                    stroke="currentColor"
                    stroke-width="2"
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    ><path
                      d="M12.22 2h-.44a2 2 0 0 0-2 2v.18a2 2 0 0 1-1 1.73l-.43.25a2 2 0 0 1-2 0l-.15-.08a2 2 0 0 0-2.73.73l-.22.38a2 2 0 0 0 .73 2.73l.15.1a2 2 0 0 1 1 1.72v.51a2 2 0 0 1-1 1.74l-.15.09a2 2 0 0 0-.73 2.73l.22.38a2 2 0 0 0 2.73.73l.15-.08a2 2 0 0 1 2 0l.43.25a2 2 0 0 1 1 1.73V20a2 2 0 0 0 2 2h.44a2 2 0 0 0 2-2v-.18a2 2 0 0 1 1-1.73l.43-.25a2 2 0 0 1 2 0l.15.08a2 2 0 0 0 2.73-.73l.22-.39a2 2 0 0 0-.73-2.73l-.15-.08a2 2 0 0 1-1-1.74v-.5a2 2 0 0 1 1-1.74l.15-.09a2 2 0 0 0 .73-2.73l-.22-.38a2 2 0 0 0-2.73-.73l-.15.08a2 2 0 0 1-2 0l-.43-.25a2 2 0 0 1-1-1.73V4a2 2 0 0 0-2-2z"
                    ></path><circle cx="12" cy="12" r="3"></circle></svg
                  >
                  Layanan Jasa
                  <svg
                    class="hs-dropdown-open:-rotate-180 md:hs-dropdown-open:rotate-0 ms-auto size-4 shrink-0 duration-300 md:ms-1"
                    xmlns="http://www.w3.org/2000/svg"
                    width="24"
                    height="24"
                    viewBox="0 0 24 24"
                    fill="none"
                    stroke="currentColor"
                    stroke-width="2"
                    stroke-linecap="round"
                    stroke-linejoin="round"><path d="m6 9 6 6 6-6"></path></svg
                  >
                </button>

                <div
                  class="hs-dropdown-menu hs-dropdown-open:opacity-100 relative start-0 top-full z-10 hidden w-full min-w-60 opacity-0 transition-[opacity,margin] duration-[0.1ms] before:absolute before:start-0 before:-top-5 before:h-5 before:w-full md:duration-[150ms]"
                  role="menu"
                  aria-orientation="vertical"
                  aria-labelledby="hs-header-base-mega-menu-fullwidth"
                >
                  <div
                    class="before:light:md:bg-white before:backdrop-blur-lg md:mx-6 md:rounded-lg md:bg-white md:shadow-md lg:mx-8 dark:md:bg-(--color-deep-space-blue-950)"
                  >
                    <!-- Grid -->
                    <div class="gap-4 py-1 md:grid md:grid-cols-2 md:p-2 lg:grid-cols-3">
                      <div class="flex flex-col">
                        <!-- Link -->
                        <a
                          class="flex gap-x-4 rounded-lg p-3 hover:bg-gray-100 focus:bg-gray-100 focus:outline-hidden dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
                          href="/jasa/website-development"
                        >
                          <svg
                            class="mt-1 size-4 shrink-0 dark:text-neutral-300"
                            xmlns="http://www.w3.org/2000/svg"
                            width="24"
                            height="24"
                            viewBox="0 0 24 24"
                            fill="none"
                            stroke="currentColor"
                            stroke-width="1.5"
                            stroke-linecap="round"
                            stroke-linejoin="round"
                          >
                            <path
                              d="M3 10V18C3 19.1046 3.89543 20 5 20H9M3 10V6C3 4.89543 3.89543 4 5 4H19C20.1046 4 21 4.89543 21 6V10M3 10H21M21 10V13"
                            ></path>
                            <path d="M15 16L13 18L15 20"></path>
                            <path d="M19 16L21 18L19 20"></path>
                          </svg>
                          <div class="grow">
                            <p
                              class="text-sm font-semibold text-(--color-ink-black-800) dark:text-neutral-300"
                            >
                              Website Development
                            </p>
                            <p class="text-sm text-(--color-ink-black-700) dark:text-neutral-400">
                              Website profesional, cepat, dan aman untuk meningkatkan citra dan
                              performa bisnis Anda.
                            </p>
                          </div>
                        </a>
                        <!-- End Link -->

                        <!-- Link -->
                        <a
                          class="flex gap-x-4 rounded-lg p-3 hover:bg-gray-100 focus:bg-gray-100 focus:outline-hidden dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
                          href="/jasa/it-consulting"
                        >
                          <svg
                            class="mt-1 size-4 shrink-0 dark:text-neutral-300"
                            xmlns="http://www.w3.org/2000/svg"
                            width="24"
                            height="24"
                            viewBox="0 0 24 24"
                            fill="none"
                            stroke="currentColor"
                            stroke-width="1.5"
                            stroke-linecap="round"
                            stroke-linejoin="round"
                          >
                            <path d="M16 21v-2a4 4 0 0 0-4-4H6a4 4 0 0 0-4 4v2"></path>
                            <circle cx="9" cy="7" r="4"></circle>
                            <path d="M22 21v-2a4 4 0 0 0-3-3.87"></path>
                            <path d="M16 3.13a4 4 0 0 1 0 7.75"></path></svg
                          >
                          <div class="grow">
                            <p
                              class="text-sm font-semibold text-(--color-ink-black-800) dark:text-neutral-300"
                            >
                              IT Consulting
                              <span
                                class="inline-flex items-center gap-x-1 rounded-full bg-red-100 px-1.5 py-1 text-xs font-medium text-red-800 dark:bg-red-500/10 dark:text-red-500"
                              >
                                <svg
                                  class="size-3 shrink-0"
                                  xmlns="http://www.w3.org/2000/svg"
                                  width="24"
                                  height="24"
                                  viewBox="0 0 24 24"
                                  fill="none"
                                  stroke="currentColor"
                                  stroke-width="2"
                                  stroke-linecap="round"
                                  stroke-linejoin="round"
                                >
                                  <path
                                    d="m21.73 18-8-14a2 2 0 0 0-3.48 0l-8 14A2 2 0 0 0 4 21h16a2 2 0 0 0 1.73-3Z"
                                  ></path>
                                  <path d="M12 9v4"></path>
                                  <path d="M12 17h.01"></path>
                                </svg>
                                Gratis
                              </span>
                            </p>
                            <p class="text-sm text-(--color-ink-black-700) dark:text-neutral-400">
                              Konsultasi IT strategis untuk optimasi sistem, efisiensi operasional,
                              dan percepatan transformasi digital.
                            </p>
                          </div>
                        </a>
                        <!-- End Link -->

                        <!-- Link -->
                        <a
                          class="flex gap-x-4 rounded-lg p-3 hover:bg-gray-100 focus:bg-gray-100 focus:outline-hidden dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
                          href="/jasa/data-system-migration"
                        >
                          <svg
                            class="mt-1 size-4 shrink-0 dark:text-neutral-300"
                            xmlns="http://www.w3.org/2000/svg"
                            width="24"
                            height="24"
                            viewBox="0 0 24 24"
                            fill="none"
                            stroke="currentColor"
                            stroke-width="1.5"
                            stroke-linecap="round"
                            stroke-linejoin="round"
                          >
                            <ellipse cx="12" cy="5" rx="9" ry="3"></ellipse>
                            <path d="M3 5v14a9 3 0 0 0 18 0V5"></path>
                            <path d="M3 12a9 3 0 0 0 18 0"></path></svg
                          >
                          <div class="grow">
                            <p
                              class="text-sm font-semibold text-(--color-ink-black-800) dark:text-neutral-300"
                            >
                              Data & System Migration
                            </p>
                            <p class="text-sm text-(--color-ink-black-700) dark:text-neutral-400">
                              Layanan migrasi data aman dan minim downtime untuk mendukung
                              kelancaran operasional perusahaan.
                            </p>
                          </div>
                        </a>
                        <!-- End Link -->
                      </div>
                      <!-- End Col -->

                      <div class="flex flex-col">
                        <!-- Link -->
                        <a
                          class="flex gap-x-4 rounded-lg p-3 hover:bg-gray-100 focus:bg-gray-100 focus:outline-hidden dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
                          href="/jasa/cloud-hosting"
                        >
                          <svg
                            class="mt-1 size-4 shrink-0 dark:text-neutral-300"
                            xmlns="http://www.w3.org/2000/svg"
                            width="24"
                            height="24"
                            viewBox="0 0 24 24"
                            fill="none"
                            stroke="currentColor"
                            stroke-width="1.5"
                            stroke-linecap="round"
                            stroke-linejoin="round"
                          >
                            <path d="M17.5 19H9a7 7 0 1 1 6.71-9h1.79a4.5 4.5 0 1 1 0 9Z"
                            ></path></svg
                          >
                          <div class="grow">
                            <p
                              class="text-sm font-semibold text-(--color-ink-black-800) dark:text-neutral-300"
                            >
                              Cloud Hosting & Infrastructure
                            </p>
                            <p class="text-sm text-(--color-ink-black-700) dark:text-neutral-400">
                              Solusi cloud hosting scalable dan aman untuk meningkatkan
                              fleksibilitas infrastruktur IT Anda.
                            </p>
                          </div>
                        </a>
                        <!-- End Link -->

                        <!-- Link -->
                        <a
                          class="flex gap-x-4 rounded-lg p-3 hover:bg-gray-100 focus:bg-gray-100 focus:outline-hidden dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
                          href="/jasa/it-procurement-support"
                        >
                          <svg
                            class="mt-1 size-4 shrink-0 dark:text-neutral-300"
                            xmlns="http://www.w3.org/2000/svg"
                            width="24"
                            height="24"
                            viewBox="0 0 24 24"
                            fill="none"
                            stroke="currentColor"
                            stroke-width="1.5"
                            stroke-linecap="round"
                            stroke-linejoin="round"
                            ><path d="m7.5 4.27 9 5.15"></path><path
                              d="M21 8a2 2 0 0 0-1-1.73l-7-4a2 2 0 0 0-2 0l-7 4A2 2 0 0 0 3 8v8a2 2 0 0 0 1 1.73l7 4a2 2 0 0 0 2 0l7-4A2 2 0 0 0 21 16Z"
                            ></path><path d="m3.3 7 8.7 5 8.7-5"></path><path d="M12 22V12"
                            ></path></svg
                          >
                          <div class="grow">
                            <p
                              class="text-sm font-semibold text-(--color-ink-black-800) dark:text-neutral-300"
                            >
                              IT Procurement & Support
                            </p>
                            <p class="text-sm text-(--color-ink-black-700) dark:text-neutral-400">
                              Pengadaan perangkat IT terpercaya dengan proses efisien, transparan,
                              dan sesuai kebutuhan bisnis.
                            </p>
                          </div>
                        </a>
                        <!-- End Link -->

                        <!-- Link -->
                        <a
                          class="flex gap-x-4 rounded-lg p-3 hover:bg-gray-100 focus:bg-gray-100 focus:outline-hidden dark:text-neutral-300 dark:hover:bg-neutral-700 dark:focus:bg-neutral-700"
                          href="/jasa/saas-partners"
                        >
                          <svg
                            class="mt-1 size-4 shrink-0 dark:text-neutral-300"
                            xmlns="http://www.w3.org/2000/svg"
                            width="24"
                            height="24"
                            viewBox="0 0 24 24"
                            fill="none"
                            stroke="currentColor"
                            stroke-width="1.5"
                            stroke-linecap="round"
                            stroke-linejoin="round"
                            ><path d="M7 10v12"></path><path
                              d="M15 5.88 14 10h5.83a2 2 0 0 1 1.92 2.56l-2.33 8A2 2 0 0 1 17.5 22H4a2 2 0 0 1-2-2v-8a2 2 0 0 1 2-2h2.76a2 2 0 0 0 1.79-1.11L12 2h0a3.13 3.13 0 0 1 3 3.88Z"
                            ></path></svg
                          >
                          <div class="grow">
                            <p
                              class="text-sm font-semibold text-(--color-ink-black-800) dark:text-neutral-300"
                            >
                              SaaS Partners
                            </p>
                            <p class="text-sm text-(--color-ink-black-700) dark:text-neutral-400">
                              Solusi SaaS premium yang terintegrasi untuk mendukung produktivitas,
                              keamanan, dan pertumbuhan bisnis.
                            </p>
                          </div>
                        </a>
                        <!-- End Link -->
                      </div>
                      <!-- End Col -->

                      <div class="mt-2 flex flex-col md:mt-0">
                        <span
                          class="ms-2.5 mt-3 mb-2 text-sm font-semibold text-(--color-ink-black-800) dark:text-neutral-300"
