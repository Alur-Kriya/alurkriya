---
import BaseLayout from '@layouts/BaseLayout.astro';
import HeroBanner from '@components/services/HeroBanner.astro';
import Partners from '@components/Partners.astro';
import Approach from '@components/services/Approach.astro';
import Faq from '@components/Faq.astro';
import RichSnipetts from '@components/RichSnippets.astro';
import srvcA from '@images/srvc-a.png';

const content = {
  heading: 'Layanan Kami',
  description: 'Temukan solusi teknologi terbaik untuk mendukung pertumbuhan bisnis Anda.',
  images: srvcA,
};
---

<BaseLayout>
  <HeroBanner {...content} />
  <Partners />
  <Approach />
  <Faq />
  <RichSnipetts />
</BaseLayout>
